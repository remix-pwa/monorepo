{
  "name": "@remix-pwa/client",
  "version": "2.0.0",
  "description": "A set of utilities for client-side development to enhance the native feel of your Remix App",
  "repository": {
    "type": "git",
    "url": "git@github.com:remix-pwa/monorepo",
    "directory": "packages/client"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "TIMING=1 eslint --fix \"lib/**/*.ts\"",
    "prepublish": "npm run build",
    "test": "vitest --run --environment node --threads --single-thread --pass-with-no-tests"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@remix-pwa/eslint-config": "^0.0.0",
    "@remix-pwa/lint-staged-config": "^0.0.0"
  }
}
