    "@remix-pwa/dev": "^0.0.0",
    "@remix-pwa/eslint-config": "^0.0.0",
    "@remix-pwa/lint-staged-config": "^0.0.0",
    "@types/jest": "^29.5.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  }
