{
  "name": "@remix-pwa/worker-runtime",
  "version": "0.0.0",
  "description": "A vanilla JavaScript worker runtime for remix service worker",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/src/service-worker.internal.js",
  "engines": {
    "node": ">=18.0.0"
  },
